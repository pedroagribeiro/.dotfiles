{
    "window.commandCenter": true,
    "window.menuBarVisibility": "toggle",
    "workbench.colorTheme": "Ros√© Pine (no italics)",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "right",
    "files.autoSave": "onFocusChange",
    "editor.renderWhitespace": "trailing",
    "editor.cursorStyle": "block",
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "editor.fontFamily": "MesloLGL Nerd Font",
    "editor.fontSize": 15,
    "editor.experimentalInlineEdit.fontFamily": "MesloLGL Nerd Font",
    "editor.inlineSuggest.fontFamily": "MesloLGL Nerd Font",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "debug.console.fontFamily": "MesloLGL Nerd Font",
    "terminal.integrated.fontFamily": "MesloLGL Nerd Font",
    "terminal.integrated.fontSize": 15,
    "prettier.configPath": ".prettierrc.mjs",
    "go.toolsManagement.autoUpdate": true,
    "[go]": {
      "editor.defaultFormatter": "golang.go"
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "thunder-client.saveToWorkspace": true
}